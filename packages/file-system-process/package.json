{
  "name": "@lvce-editor/file-system-process",
  "version": "0.0.0-dev",
  "description": "File System Process",
  "keywords": [
    "Lvce Editor",
    "File System"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvce-editor/file-system-process.git"
  },
  "license": "MIT",
  "author": "Lvce Editor",
  "type": "module",
  "main": "src/fileSystemProcessMain.ts",
  "bin": {
    "file-system-process": "bin/fileSystemProcess.js"
  },
  "scripts": {
    "test": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch"
  },
  "jest": {
    "injectGlobals": false,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50
      }
    }
  },
  "dependencies": {
    "@lvce-editor/assert": "^1.5.1",
    "@lvce-editor/rpc": "^5.4.0",
    "@lvce-editor/verror": "^1.7.0",
    "trash": "^10.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@lvce-editor/rpc-registry": "^7.2.0",
    "@types/node": "^25.0.7",
    "electron": "^39.2.7",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6"
  },
  "engines": {
    "node": ">=22"
  }
}
