{
  "name": "@lvce-editor/file-system-process",
  "version": "0.0.0-dev",
  "description": "File System Process",
  "keywords": [
    "Lvce Editor",
    "File System"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvce-editor/file-system-process.git"
  },
  "license": "MIT",
  "author": "Lvce Editor",
  "type": "module",
  "main": "src/fileSystemProcessMain.ts",
  "bin": {
    "file-system-process": "bin/fileSystemProcess.js"
  },
  "scripts": {
    "test": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch"
  },
  "jest": {
    "injectGlobals": false,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 3,
        "functions": 3,
        "lines": 3
      }
    }
  },
  "dependencies": {
    "@lvce-editor/assert": "^1.3.0",
    "@lvce-editor/json-rpc": "^5.4.0",
    "@lvce-editor/rpc": "^1.27.0",
    "@lvce-editor/verror": "^1.6.0",
    "execa": "^9.5.2",
    "open": "^10.1.1",
    "tail": "^2.2.6",
    "tmp-promise": "^3.0.3",
    "trash": "^9.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/debug": "^4.1.12",
    "@types/is-object": "^1.0.5",
    "@types/node": "^20.11.24",
    "@types/tail": "^2.2.3",
    "@types/tar-fs": "^2.0.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2"
  },
  "engines": {
    "node": ">=22"
  }
}
